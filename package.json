{
  "name": "purescript-behaviors-three-example",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "",
  "author": "",
  "license": "Private",
  "private": true,
  "scripts": {
    "postinstall": "rm -Rf .psc-package && $(yarn bin)/psc-package install",
    "clean": "rm -Rf .pulp-cache dist output",
    "pscid:build": "$(yarn bin)/pulp build",
    "build": "yarn bin clean && $(yarn bin)/pulp build && $(yarn bin)/parcel build index.html --experimental-scope-hoisting",
    "serve": "$(yarn bin)/pulp --watch build & $(yarn bin)/parcel index.html --open",
    "id": "$(yarn bin)/pscid",
    "test": "$(yarn bin)/pulp test"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.3",
    "psc-package": "^0.3.2",
    "pscid": "^2.6.0",
    "pulp": "^12.3.0",
    "purescript": "^0.12.1",
    "purescript-psa": "^0.7.3"
  },
  "dependencies": {
    "three": "^0.98.0"
  }
}
